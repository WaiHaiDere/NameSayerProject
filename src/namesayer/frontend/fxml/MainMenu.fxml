<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" maxHeight="500.0" maxWidth="700.0" minHeight="500.0" minWidth="700.0" prefHeight="500.0" prefWidth="700.0" spacing="40.0" style="-fx-background-color: #373737;" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="namesayer.backend.menus.MainMenu">
   <children>
      <Label text="Name Sayer" textFill="#c0b283">
         <font>
            <Font name="System Bold" size="60.0" />
         </font>
         <VBox.margin>
            <Insets bottom="40.0" />
         </VBox.margin>
      </Label>
      <Button fx:id="practiceBtn" mnemonicParsing="false" onAction="#practiceBtnClicked" prefHeight="40.0" prefWidth="200.0" style="-fx-background-color: #c0b283;" text="Practice" textFill="#373737">
         <font>
            <Font size="20.0" />
         </font>
         <graphic>
            <ImageView fitHeight="28.0" fitWidth="58.0" pickOnBounds="true" preserveRatio="true" translateX="-10.0">
               <image>
                  <Image url="@../../resources/img_practice.png" />
               </image>
            </ImageView>
         </graphic>
         <padding>
            <Insets left="-20.0" />
         </padding>
      </Button>
      <Button fx:id="namesBtn" alignment="CENTER" mnemonicParsing="false" onAction="#namesBtnClicked" prefHeight="40.0" prefWidth="200.0" style="-fx-background-color: #c0b283;" text="Names" textFill="#373737">
         <font>
            <Font size="20.0" />
         </font>
         <graphic>
            <ImageView fitHeight="28.0" fitWidth="58.0" pickOnBounds="true" preserveRatio="true" translateX="-17.5">
               <image>
                  <Image url="@../../resources/img_database.png" />
               </image>
            </ImageView>
         </graphic>
         <padding>
            <Insets left="-15.0" />
         </padding>
      </Button>
      <Button fx:id="helpBtn" mnemonicParsing="false" onAction="#helpBtnClicked" prefHeight="40.0" prefWidth="200.0" style="-fx-background-color: #c0b283;" text="Help" textFill="#373737">
         <font>
            <Font size="20.0" />
         </font>
         <graphic>
            <ImageView fitHeight="28.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true" translateX="-20.0">
               <image>
                  <Image url="@../../resources/img_help.png" />
               </image>
            </ImageView>
         </graphic>
         <padding>
            <Insets left="-20.0" />
         </padding>
      </Button>
      <Button fx:id="quitBtn" mnemonicParsing="false" onAction="#quitBtnClicked" prefHeight="40.0" prefWidth="200.0" style="-fx-background-color: #c0b283;" text="Quit" textFill="#373737">
         <font>
            <Font size="20.0" />
         </font>
         <graphic>
            <ImageView fitHeight="28.0" fitWidth="58.0" pickOnBounds="true" preserveRatio="true" translateX="-20.0">
               <image>
                  <Image url="@../../resources/img_quit.png" />
               </image>
            </ImageView>
         </graphic>
         <padding>
            <Insets left="-20.0" />
         </padding>
      </Button>
   </children>
</VBox>
