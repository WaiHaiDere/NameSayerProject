<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox maxHeight="500.0" maxWidth="700.0" minHeight="500.0" minWidth="700.0" prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nameSayer.MainMenu">
   <children>
      <Pane maxHeight="100.0" maxWidth="700.0" minHeight="100.0" minWidth="700.0" prefHeight="100.0" prefWidth="700.0">
         <children>
            <Label layoutX="175.0" layoutY="5.0" text="Name Sayer">
               <font>
                  <Font name="System Bold" size="62.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Pane maxHeight="150.0" maxWidth="700.0" minHeight="150.0" minWidth="700.0" prefHeight="150.0" prefWidth="700.0">
         <children>
            <Button fx:id="practiceMenuButton" layoutX="150.0" layoutY="30.0" maxHeight="90.0" maxWidth="400.0" minHeight="90.0" minWidth="400.0" mnemonicParsing="false" onAction="#handlePracticeMenuPressed" prefHeight="90.0" prefWidth="400.0" text="Practice">
               <font>
                  <Font size="41.0" />
               </font>
            </Button>
         </children>
      </Pane>
      <Pane maxHeight="150.0" maxWidth="700.0" minHeight="150.0" minWidth="700.0" prefHeight="150.0" prefWidth="700.0">
         <children>
            <Button fx:id="nameMenuButton" layoutX="150.0" layoutY="30.0" maxHeight="90.0" maxWidth="400.0" minHeight="90.0" minWidth="400.0" mnemonicParsing="false" onAction="#handleNameMenuPressed" prefHeight="90.0" prefWidth="400.0" text="Names">
               <font>
                  <Font size="41.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</VBox>
